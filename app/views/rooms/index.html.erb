<p style="color: green"><%= notice %></p>

<h1>Rooms</h1>

<div id="rooms">
  <% @rooms.each do |room| %>
    <%= render room %>
    <p>
      <%= link_to "Show this room", room %>
      <%= link_to "Edit this room", edit_room_path(room) %>
    </p>
  <% end %>
</div>

<%= link_to "New room", new_room_path %>
